                                                                                                                        "@typescript-eslint/parser": "^5.45.1",
                                                                                                                            "cypress": "^12.0.0",
                                                                                                                                "eslint": "^8.45.0",
                                                                                                                                    "eslint-config-prettier": "^8.5.0",
                                                                                                                                        "eslint-plugin-prettier": "^4.2.1",
                                                                                                                                            "husky": "^8.0.0",
                                                                                                                                                "jest": "^29.5.0",
                                                                                                                                                    "lint-staged": "^13.2.0",
                                                                                                                                                        "prettier": "^2.8.8",
                                                                                                                                                            "typescript": "^4.8.0"
                                                                                                                                                              },
