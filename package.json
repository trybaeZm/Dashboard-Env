{
      "name": "your-project-name",
        "version": "1.0.0",
          "description": "A project following trunk-based development with ESLint, Prettier, Jest, and GitHub Actions.",
            "main": "index.js",
              "scripts": {
                  "start": "node index.js",
                      "lint": "eslint 'src/**/*.{js,ts,tsx}'",
                          "prettier": "prettier --write 'src/**/*.{js,ts,tsx,json,css,md}'",
                              "test": "jest",
                                  "format": "prettier --check 'src/**/*.{js,ts,tsx,json,css,md}'"
                                    },
                                      "husky": {
                                          "hooks": {
                                                "pre-commit": "lint-staged"
                                                    }
                                                      },
                                                        "lint-staged": {
